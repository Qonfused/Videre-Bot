  "devDependencies": {
    "eslint": "^7.24.0",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "babel-plugin-module-resolver": "^4.1.0",
    "chalk": "4.1.0",
    "d3-array": "^2.12.1",
    "discord.js": "12.5.3",
    "dompurify": "^2.2.7",
    "dotenv": "8.2.0",
    "jsdom": "^16.5.3",
    "node-fetch": "^2.6.1",
    "puppeteer": "^8.0.0"
  },
  "scripts": {
    "start": "nodemon --exec babel-node src",
